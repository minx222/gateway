{
  "name": "front-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build NODE_ENV=production",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env RUNNING_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env RUNNING_ENV=prod node dist/apps/front-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/front-gateway/test/jest-e2e.json",
    "prepare": "husky install",
    "commit": "git status && git add -A && git-cz && git push"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/typeorm": "^10.0.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "esbuild-loader": "^4.2.0",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "md5": "^2.3.0",
    "mongodb": "^6.8.0",
    "mysql": "^2.18.1",
    "nanoid": "^3.3.7",
    "opossum": "^8.1.4",
    "qs": "^6.12.2",
    "radash": "^12.1.0",
    "request": "^2.88.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "urijs": "^1.19.11",
    "xml2js": "^0.6.2",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/md5": "^2.3.5",
    "@types/mongodb": "^4.0.7",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.3.1",
    "@types/opossum": "^8.1.7",
    "@types/qs": "^6.9.15",
    "@types/request": "^2.48.12",
    "@types/supertest": "^6.0.0",
    "@types/urijs": "^1.19.25",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.10",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "source-map-support": "^0.5.21",
    "speed-measure-webpack-plugin": "^1.5.0",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1"
    }
  }
}
